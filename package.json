{
  "name": "aws-course-node",
  "version": "1.0.0",
  "description": "Serverless product service using Typescript",
  "scripts": {
    "product:deploy": "cd ./product-service && sls deploy",
    "product:offline": "cd ./product-service && sls offline",
    "import:deploy": "cd ./import-service && sls deploy",
    "import:offline": "cd ./import-service && sls offline",
    "authorization:deploy": "cd ./authorization-service && sls deploy",
    "all:deploy": "npm run authorization:deploy && npm run product:deploy && npm run import:deploy",
    "test": "jest --colors"
  },
  "dependencies": {
    "aws-sdk": "^2.790.0",
    "csv-parser": "^2.3.3",
    "joi": "^17.3.0",
    "pg": "^8.4.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^26.0.15",
    "@types/node": "^10.12.18",
    "@types/serverless": "^1.72.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "aws-sdk-mock": "^5.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.6.1",
    "serverless": "^2.9.0",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
